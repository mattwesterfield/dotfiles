!! URxvt Appearance
! Lo-res
! URxvt.font: xft:Ubuntu Mono:style=Regular:size=10
! URxvt.boldFont: xft:Ubuntu Mono:style=Bold:size=10
! URxvt.italicFont: xft:Ubuntu Mono:style=Italic:size=10
! URxvt.boldItalicfont: xft:Ubuntu Mono:style=BoldItalic:size=10
! Hi-res
URxvt.font: xft:Ubuntu Mono:style=Regular:size=15
URxvt.boldFont: xft:Ubuntu Mono:style=Bold:size=15
URxvt.italicFont: xft:Ubuntu Mono:style=Italic:size=15
URxvt.boldItalicfont: xft:Ubuntu Mono:style=BoldItalic:size=15
! Super zoom
! URxvt.font: xft:Ubuntu Mono:style=Regular:size=25
! URxvt.boldFont: xft:Ubuntu Mono:style=Bold:size=25
! URxvt.italicFont: xft:Ubuntu Mono:style=Italic:size=25
! URxvt.boldItalicfont: xft:Ubuntu Mono:style=BoldItalic:size=25
URxvt.perl-ext-common: default,tabbed,matcher,resize-font,-tabbed

URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 20
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 32
URxvt.background: [80]#141c21
URxvt.iso14755: false
URxvt.iso14755_52: false

! Restore Ctrl+Shift+(c|v)
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

Emacs.alpha: 50
